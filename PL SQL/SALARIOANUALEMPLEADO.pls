CREATE OR REPLACE FUNCTION SALARIOANUALEMPLEADO  (P_NUM_EMPLEADO EMPLOYEES.EMPLOYEE_ID%TYPE)
RETURN VARCHAR2 AS 
    V_SALARIO_ANUAL EMPLOYEES.SALARY%TYPE;
BEGIN
    SELECT (SALARY+(NVL(COMMISSION_PCT,0)*SALARY/100))*14 INTO V_SALARIO_ANUAL
    FROM EMPLOYEES
    WHERE EMPLOYEE_ID=P_NUM_EMPLEADO;
    
  RETURN v_salario_anual;
  
END SALARIOANUALEMPLEADO;